<div class="block">

	<div class="block_head">
		<div class="bheadl"></div>
		<div class="bheadr"></div>

		<h2><%= t "license.show"%></h2>
		<ul>
			<li class="nobg">
				<%= link_to t('license.license_list'), licenses_path %>
			</li>
		</ul>
	</div>
	<div class="block_content">
		<% if notice %>
		<div class="message success">
			<p>
				<%= notice %>
			</p>
		</div>
		<% end %>
		
		<p>
		  <b><%= t "license.custom_name" %>:</b>
		  <%= @license.custom_name %>
		</p>
		
		<p>
		  <b><%= t "license.applicant" %>:</b>
		  <%= @license.applicant %>
		</p>
		
		<p>
		  <b><%= t "license.publish_reason" %>:</b>
		  <%= @license.publish_reason %>
		</p>
		
		<p>
		  <b><%= t "license.publish_content" %>:</b>
		  <%= @license.publish_content %>
		</p>
		
		<p>
		  <b><%= t "license.expires_date" %>:</b>
		  <%= @license.expires_date %>
		</p>
		
		<p>
		  <b><%= t "license.version" %>:</b>
		  <%= @license.version %>
		</p>
		
		<p>
		  <b><%= t "license.local_check_type" %>:</b>
		  <%= @license.local_check_type %>
		</p>
		
		<p>
		  <b><%= t "license.local_check" %>:</b>
		  <%= @license.local_check %>
		</p>
		
		<p>
		  <b><%= t "license.user_number" %>:</b>
		  <%= @license.user_number %>
		</p>
		
		<p>
		  <b><%= t "license.memo" %>:</b>
		  <%= @license.memo %>
		</p>
	</div>
	<div class="bendl"></div>
	<div class="bendr"></div>
</div>
